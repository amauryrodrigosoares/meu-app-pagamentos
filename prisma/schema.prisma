datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Pagamento {
  id                Int      @id @default(autoincrement())
  descricao         String
  valor             Float
  recorrente        Boolean
  parcelasPagas     Int
  totalParcelas     Int
  dataPrimeiroPagamento DateTime
}